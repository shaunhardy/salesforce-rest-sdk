{
    "name": "shaunhardy/salesforce-rest-sdk",
    "description": "An SDK for the Salesforce Rest API",
    "type": "library",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "jms/serializer": "^3.12",
        "ramsey/uuid": "^4.1",
        "doctrine/collections": "^1.6|^2.0",
        "symfony/cache": "^4.0|^5.0|^6.0|^7.0",
        "psr/log": "^1.1|^2.0|^3.0",
        "symfony/expression-language": "^4.2|^5.0|^6.0|^7.0",
        "jms/metadata": "^1.0|^2.0"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Alex Boyce",
            "email": "alex.boyce@advisorsexcel.com"
        },
        {
            "name": "playable-cn",
            "email": "developer@playable.cn"
        },
        {
            "name": "Luke Fromhold",
            "email": "hello@fromhold.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "AE\\SalesforceRestSdk\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AE\\SalesforceRestSdk\\Tests\\": ["Tests/"]
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
